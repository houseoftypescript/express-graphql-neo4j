{
  "name": "@express/neo4j",
  "version": "0.0.1",
  "description": "Express.js - GraphQL - Neo4j",
  "main": "dist/server.js",
  "repository": "git@github.com:houseoftypescript/express-graphql-neo4j.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "start": "node .",
    "start:dev": "kill-port 5000 && concurrently \"nodemon\" \"nodemon -x yarn docs\"",
    "start:prod": "node .",
    "docs": "yarn docs:swagger:json && yarn docs:swagger:yaml",
    "docs:swagger:json": "tsoa spec-and-routes",
    "docs:swagger:yaml": "json2yaml ./docs/swagger/swagger.json > ./docs/swagger/swagger.yaml",
    "lint": "eslint . --fix --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest.config.ts",
    "format": "prettier --write .",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@apollo/server": "4.7.5",
    "@graphql-tools/merge": "9.0.0",
    "@neo4j/graphql": "3.23.0",
    "axios": "1.4.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "dotenv-expand": "10.0.0",
    "express": "4.18.2",
    "graphql": "16.7.1",
    "graphql-depth-limit": "1.1.0",
    "helmet": "7.0.0",
    "http-errors": "2.0.0",
    "morgan": "1.10.0",
    "neo4j-driver": "5.10.0",
    "pino": "8.14.1",
    "pino-pretty": "10.0.1",
    "tsoa": "5.1.1"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/http-errors": "2.0.1",
    "@types/jest": "29.5.2",
    "@types/morgan": "1.9.4",
    "@types/node": "20.4.0",
    "@types/supertest": "2.0.12",
    "@types/webpack-node-externals": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "concurrently": "8.2.0",
    "eslint": "8.44.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "json2yaml": "1.1.0",
    "kill-port": "2.0.1",
    "nodemon": "2.0.22",
    "prettier": "3.0.0",
    "pretty-quick": "3.1.3",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}
